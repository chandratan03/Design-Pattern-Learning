package main;

import models.Cat;
import models.CatFoo;
import models.Dog;
import models.DogFoo;

public class Main {

	public Main() {
		Cat cat = new CatFoo();
		Dog dog = new DogFoo();
		
	}
	
	// this is the private method that require only dog can use
	private void dogScream(Dog dog) {
		dog.scream();
	}
	
	
	public static void main(String[] args) {
		new Main();
	}

}
