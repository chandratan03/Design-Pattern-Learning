package adapter;

import models.Cat;
import models.Dog;

public class CatAdapter implements Dog{
	Cat cat;
	public CatAdapter(Cat cat) {
		this.cat = cat;
	}

	@Override
	public void scream() {
		// let's just say, the dog scream == (5 times of cat meow)
		for(int i=0; i<5; i++)
			cat.meow();
	}

}
